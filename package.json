{
  "name": "midibus.js",
  "version": "0.1.2",
  "scripts": {
    "build-module": "browserify --standalone midibus index.js | uglifyjs -c -m > index.min.js",
    "build-example": "browserify example/index.js > example/bundle.js",
    "build": "npm run build-module && npm run build-example",
    "start": "budo example/index.js:bundle.js -P -l -d example"
  },
  "main": "index.js",
  "author": "pqml",
  "license": "MIT",
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "tiny-emitter": "^1.1.0"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "budo": "^9.0.0",
    "uglify-js": "^2.7.3"
  },
  "browserify": {
    "transform": []
  },
  "description": ":bus::dash::notes::notes: Web MIDI API wrapper based on themidibus"
}
